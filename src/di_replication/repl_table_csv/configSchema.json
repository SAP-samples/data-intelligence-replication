{
    "$schema": "http://json-schema.org/draft-06/schema#",
    "$id": "http://sap.com/vflow/sdi_replication.repl_table_csv.configSchema.json",
    "type": "object",
    "properties": {
        "codelanguage": {
            "type": "string"
        },
        "script": {
            "type": "string"
        },
        "debug_mode": {
            "title": "Debug mode",
            "description": "Sending debug level information to log port",
            "type": "boolean"
        },
        "drop_header": {
            "title": "Drop header",
            "description": "Drop header (not only for the first run).",
            "type": "boolean"
        },
        "only_header": {
            "title": "Only header",
            "description": "Only header (for preparation purpose).",
            "type": "boolean"
        },
        "drop_columns": {
            "title": "Drop Columns",
            "description": "List of columns to drop.",
            "type": "string"
        }
    }
}